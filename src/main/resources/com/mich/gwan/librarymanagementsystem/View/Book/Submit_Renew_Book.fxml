<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="submitRootAnchorPane" prefHeight="580.0" prefWidth="510.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mich.gwan.librarymanagementsystem.Controller.Book.SubmitRenewBookController">
   <children>
      <TextField fx:id="submitBookIdText" layoutX="66.0" layoutY="31.0" onAction="#showIssuedBookInformtion" prefHeight="25.0" prefWidth="312.0" promptText="Enter Book Id" />
      <ListView fx:id="submitBookInfoListView" layoutX="64.0" layoutY="65.0" prefHeight="449.0" prefWidth="316.0" />
      <Button fx:id="submitButton" layoutX="64.0" layoutY="538.0" mnemonicParsing="false" onAction="#submitBook" prefHeight="25.0" prefWidth="130.0" text="SUBMIT" />
      <Button fx:id="renewBookButton" layoutX="248.0" layoutY="538.0" mnemonicParsing="false" onAction="#renewBook" prefHeight="25.0" prefWidth="130.0" text="RENEW" />
      <Label alignment="CENTER" layoutX="81.0" layoutY="6.0" text="SUBMIT/RENEW BOOK" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
   </children>
</AnchorPane>
